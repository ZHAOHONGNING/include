package zhao_311;

import sun.applet.Main;

public class Student extends Thread {
	
	
	public void show1(){
		for(int i=0;i<12;i++){
			System.out.println(i);
		}
		public void show2(){
			for(int i=0;i<12;i++){
				System.out.println(i);
			}
	}
	public static void main(String[] args) {
	Student s1 = new Student("张三：");	
	Student s2 = new Student("李四：");	
	s1.show();
	s2.show();
	
	
	}

}
