package com.zhao;

import java.util.Date;

public class Order {

	private String id;//订单号
	private Date oredrDate;//订单时间
	private int totalCount;//商品中数量
	private double totalAmount;//商品总金额
	private int state;//商品状态
	private int Person;//订单所属用户
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public Date getOredrDate() {
		return oredrDate;
	}
	public void setOredrDate(Date oredrDate) {
		this.oredrDate = oredrDate;
	}
	public int getTotalCount() {
		return totalCount;
	}
	public void setTotalCount(int totalCount) {
		this.totalCount = totalCount;
	}
	public double getTotalAmount() {
		return totalAmount;
	}
	public void setTotalAmount(double totalAmount) {
		this.totalAmount = totalAmount;
	}
	public int getState() {
		return state;
	}
	public void setState(int state) {
		this.state = state;
	}
	public int getPerson() {
		return Person;
	}
	public void setPerson(int person) {
		Person = person;
	}
}
