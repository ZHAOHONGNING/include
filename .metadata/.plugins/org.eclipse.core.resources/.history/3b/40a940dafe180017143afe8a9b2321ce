package zhao_311;

import sun.applet.Main;

public class Student extends Thread {
	
	public void show(){
		for(int i=0;i<12;i++){
			System.out.println(i);
		}
	}
	public static void main(String[] args) {
	Student s1 = new Student();	
	Student s2 = new Student();	
	String p1 = s1.show();
	
	System.out.println();
	System.out.println();
	}

}
