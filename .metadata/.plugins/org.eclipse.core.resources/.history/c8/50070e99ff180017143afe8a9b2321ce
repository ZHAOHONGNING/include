package zhao_311;

import sun.applet.Main;

public class Student extends Thread {
	
	private String name;
	public Student(String name) {
		this.name = name;
	}
	public void show(){
		for(int i=0;i<12;i++){
			System.out.println(i);
		}
	}
	public static void main(String[] args) {
	Student s1 = new Student("张三：");	
	Student s2 = new Student("李四：");	
	s1.show();
	s2.show();
	
	
	}

}
